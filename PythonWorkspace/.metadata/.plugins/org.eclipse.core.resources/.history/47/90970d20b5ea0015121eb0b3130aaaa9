from sklearn import linear_model
import numpy as np

x_matrix = []
y_matrix = []


f,n = map(int,raw_input().split())
tmp=n
while tmp>0:
    inp=map(float,raw_input().split())
    x_matrix.append(inp[:f-2])
    y_matrix.append(inp[f-1])
    tmp-=1
 
lin = linear_model.LinearRegression()
lin.fit(x_matrix, y_matrix)   


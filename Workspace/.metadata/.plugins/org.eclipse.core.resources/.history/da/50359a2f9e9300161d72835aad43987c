class Base
{
    public void display()
    {
        System.out.println("hack");
    }
}

class Derived extends Base
{
    public void display()
    {
        System.out.println("Derived");
    }
}

public class M
{
    public static void main(String args[])
    {
        Base d=new Derived();
        d.display();
    }
}
package hacker.rank;

import java.util.Arrays;
import java.util.Scanner;

public class MorganAndString {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tst = Integer.parseInt(sc.nextLine());
		while (tst > 0) {

			StringBuilder t1 = sc.nextLine().toCharArray();
			StringBuilder t2 = sc.nextLine().toCharArray();

			StringBuilder res = new StringBuilder("");
			int i, j;
			for (i = 0, j = 0; i < s1.length && j < s2.length;) {
				if (s1[i] < s2[j])
					res.append(s1[i++]);
				else if (s1[i] > s2[j])
					res.append(s2[j++]);
				else {
					int temp_i = i;
					int temp_j = j;
					while (s1[temp_i] == s2[temp_j]) {
						temp_i++;
						temp_j++;
					}
				}
			}
			if (i == s1.length) {
				for (; j < s2.length; j++)
					res.append(s2[j]);

			} else if (j == s2.length) {
				for (; i < s1.length; i++)
					res.append(s1[i]);

			}
			System.out.println(res);
			tst--;
		}
	}

}
import java.util.*;

class Line{
	int m;
	int c;
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + c;
		result = prime * result + m;
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Line other = (Line) obj;
		if (c != other.c)
			return false;
		if (m != other.m)
			return false;
		return true;
	}
	
}
public class LazerGun {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		Set<Line> set=new HashSet<Line>();
		int n=sc.nextInt();
		int x0=sc.nextInt();
		int y0=sc.nextInt();
		for (int i = 0; i < n; i++) {
			int x1=sc.nextInt();
			int y1=sc.nextInt();
			
			
			
		}
		
		
	}

}

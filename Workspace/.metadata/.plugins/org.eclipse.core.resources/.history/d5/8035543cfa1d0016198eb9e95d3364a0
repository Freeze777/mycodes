package algo;

public class RotateArray90 {
	public static void main(String[] args) {
		int row = 4;
		int col = 4;
		int[][] mat = new int[row][col];

		for (int i = 0; i < mat.length; i++)
			for (int j = 0; j < mat[0].length; j++)
				mat[i][j] = (int) (Math.random() * 101);

		GlobalUtils.printArray(mat);
		
		for (int i = 0; i < mat.length; i++)
			for (int j = 0; j < mat[0].length; j++)
				mat[i][j] = mat[j][i];
		
		GlobalUtils.printArray(mat);
		
	}

}

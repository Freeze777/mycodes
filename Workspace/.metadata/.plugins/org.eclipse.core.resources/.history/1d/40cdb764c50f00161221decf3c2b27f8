import java.util.Scanner;


public class Multiple3and5 {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int tst=sc.nextInt();
		
		while(tst>0){
			
			long n=sc.nextLong();
			long an_3=n-n%3;
			long an_5=n-n%5;
			long an_15=n-n%15;
			
			long sum3=(n*(an_3))>>2;
			long sum5=(n*(an_5))>>2;
			long sum15=(n*(an_15))>>2;
			
			long sum=sum15+sum3+sum5;
			
			System.out.println(sum);
			
			
			
			
			tst--;
		}
		
	}

}

import java.util.*;

public class LazerGun {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Set<Double> slopes = new HashSet<Double>();
		int n = sc.nextInt();
		int x0 = sc.nextInt();
		int y0 = sc.nextInt();
		for (int i = 0; i < n; i++) {
			int x1 = sc.nextInt();
			int y1 = sc.nextInt();
			set.add(new Line(x0, y0, x1, y1));
		}
		System.out.println(set.size());

	}

}

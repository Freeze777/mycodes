import java.util.Scanner;

public class LatticePaths {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int row=500,col=500;
		long mod=1000000007;
		long dp[][]=new long[row][col];
		for (int i =1; i < dp.length; i++) {
			dp[0][i]=1;
			dp[i][0]=1;
		}
		
		for (int i = 1; i < row; i++) {
			for (int j =1; j < col; j++) {
				dp[i][j]=dp[i-1][j]+dp[j][j]
			}
		}
		
		
		
	}
}

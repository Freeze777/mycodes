				nums[i++]=(ch-'0');
			}
        	
        
        	
        	for (int x = 0; x < nums.length-k; x++) {
        		int temp=0;
        		while(curr!=0)
        			curr*=nums[]
        	}
        	
        	
        	tst--;
        }
        
    }
}
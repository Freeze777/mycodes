from numpy import ndarray

count = ndarray((26,),int)
count.fill(0)
s=raw_input()

for c in s:
    index=ord(c)-ord('a')
    count[index]+=1;

count2alpha={}
for i in range(len(count)):
    if count[i] in count2alpha:
        count2alpha[count[i]]+=1
    else:
        count2alpha[count[i]]=1
        
    
print count2alpha 
    

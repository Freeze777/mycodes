package algo.backtracking;

public class DecreasingPathMatrix {
	public static void main(String[] args) {
		int[][] arr={
				{100,120,85,90,20},
				{80,90,110,80,70},
				{75,100,85,100,75},
				{90,70,120,80,70},
				{100,60,65,60,50}				
		};
		System.out.println(countPaths(arr,0,0));
	}

	private static int countPaths(int[][] arr,int si,int sj) {
		int[] count={0};
		dfs()
		return count[0];
	}
}

import java.util.*;

class TestClass {
	public static void main(String args[]) throws Exception {
		Scanner s = new Scanner(System.in);
		int l = s.nextInt();
		int m = s.nextInt();
		char[] str = s.next().toCharArray();
		
		if (m >= str.length) {
			System.out.println("");
			System.exit(0);
		}
		int start = 0;
		while (m > 0) {
			char max = str[start];
			int max_loc = start;
			for (int i =0; i < m; i++) {
				if (str[start+i] > max) {
					max_loc = start+i;
					max = str[start+i];
				}
			}
			System.out.println(max_loc);
			System.out.println(start);
			for (int i = start; i < max_loc; i++) {
				str[i] = '!';
			}
			for (int i = 0; i < str.length; i++) {
				System.out.print(str[i]);
			}
			System.out.println();
			m -= (max_loc - start);
			start = max_loc+1;
		}
		
	}
}

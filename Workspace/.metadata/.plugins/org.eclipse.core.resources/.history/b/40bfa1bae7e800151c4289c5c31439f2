import java.io.*;
import java.util.*;

public class Soln {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. PrLong output to STDOUT. Your class should be named Solution. */
    Scanner sc=new Scanner(System.in);
    
    Long a=sc.nextLong();
    Long b=sc.nextLong();
    
    Long min=Math.min(a,b);
    Long max=Math.max(a, b);
    
    a=(Long)Math.ceil(Math.cbrt(min));
    b=(Long)Math.floor(Math.cbrt(max));
    
    long sum1=(a*(a+1))/2;
    sum1*=sum1;
    long sum2=(b*(b+1))/2;
    sum2*=sum2;
    
    long sum=Math.abs(sum1-sum2)+(a*a*a);
    double ans=(double)sum/Math.abs(b-a+1);
    System.out.println(ans);
    
    
    
    
    }
}
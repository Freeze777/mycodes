package project.elective;

import gurobi.*;

public class myTSP {

	public static void main(String args[]) {

		try {
			int distance[][] = { { Integer.MAX_VALUE, 10, 8, 9, 7 },
					{ 10, Integer.MAX_VALUE, 10, 5, 6 },
					{ 8, 10, Integer.MAX_VALUE, 8, 9 },
					{ 9, 5, 8, Integer.MAX_VALUE, 6 },
					{ 7, 6, 9, 6, Integer.MAX_VALUE } };
			int nCities = distance.length;

			GRBEnv env = new GRBEnv();
			GRBModel model = new GRBModel(env);
			model.set(GRB.StringAttr.ModelName, "5-city TSP");
			
			int nVariables=nCities*nCities;
			GRBVar[][] xij = new GRBVar[nCities][nCities];
			for (int i = 0; i < nCities; i++) {
				for (int j = 0; j < nCities; ++j) {
			        xij[i][j] =model.addVar(0.0,1.0,distance[i][j], GRB.BINARY,"x"+i+" "+j);
			        xij[j][i]=xij[i][j];
			      }
				
			}
			for (int i = 0; i < n; i++)
		        for (int j = 0; j <= i; j++) {
		          vars[i][j] = model.addVar(0.0, 1.0, distance(x, y, i, j),
		                                    GRB.BINARY,
		                                  "x"+String.valueOf(i)+"_"+String.valueOf(j));
		          vars[j][i] = vars[i][j];
		        }
 
		} catch (GRBException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}

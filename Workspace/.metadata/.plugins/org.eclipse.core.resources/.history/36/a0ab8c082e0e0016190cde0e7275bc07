import java.util.Arrays;
import java.util.Scanner;


public class Triplets {
	
	
		
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int d=sc.nextInt();
		int[] arr=new int[n];
		for (int i = 0; i < n; i++) {
			arr[i]=sc.nextInt();
		}
		
		for (int i = 0; i < arr.length; i++) {
			int a=arr[i];
			int b=arr[i+1];
			int c=arr[i+2];
			
			if(((a+c)==2*b)&&(b-a==c-b)&& (c-b==d))
		}
		
		
	}
	 

}

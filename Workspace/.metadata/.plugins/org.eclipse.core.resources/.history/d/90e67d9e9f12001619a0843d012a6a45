import java.util.Scanner;


public class PythagorianTriplets {
	public static void main(String[] args) {
		int a=3,b=4,c=5;
		Scanner sc=new Scanner(System.in);
		int tst=sc.nextInt();
		int tst=
		generatePythagorianTriplets(a,b,c);
		
	}
	/*
	1  -2  2 
	2  -1  2 
	2  -2  3
	
	-1  2  2
	-2  1  2
	-2  2  3
	
	1  2  2 
	2  1  2
	2  2  3
	

	*/
	private static void generatePythagorianTriplets(int a, int b, int c) {
		
		int a1=a+(-2*b)+(2*c);
		int b1=(2*a)+(-1*b)+(2*c);
		int c1=(2*a)+(-2*b)+(3*c);
		System.out.println("["+a1+","+b1+","+c1+"]");
		generatePythagorianTriplets(a1,b1,c1);
		 
		 a1=(-1*a)+(2*b)+(2*c);
		 b1=(-2*a)+(1*b)+(2*c);
		 c1=(-2*a)+(2*b)+(3*c);
		 System.out.println("["+a1+","+b1+","+c1+"]");
		 generatePythagorianTriplets(a1,b1,c1);
		 
		 a1=a+(2*b)+(2*c);
		 b1=(2*a)+(1*b)+(2*c);
		 c1=(2*a)+(2*b)+(3*c);
		 System.out.println("["+a1+","+b1+","+c1+"]");
		 generatePythagorianTriplets(a1,b1,c1);
		
		
	}

}

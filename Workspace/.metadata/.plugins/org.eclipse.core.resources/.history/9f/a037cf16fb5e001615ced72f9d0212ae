import java.util.Scanner;

class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while (t-- > 0) {
			int n = sc.nextInt();
			long k = sc.nextLong();
			long[] cost = new long[n];
			for (int i = 0; i < cost.length; i++) {
				cost[i] = sc.nextLong();
			}
			
		}
	}
}

public class SuperValidBracketSequence {
	static long mod = 1000000007;
	static long[] catalan = new long[2001];

	private static void generate() {
		catalan[0] = 1;
		catalan[1] = 1;
		for (int i = 2; i < catalan.length; i++) {
			for (int j = 1; j <= i; j++) {
				long temp = (catalan[j - 1] * catalan[i - j]) % mod;
				catalan[i] += temp;
				catalan[i] %= mod;
			}
		}

	}
}

package algo.trees;

public class NumberOfBSTOfNkeys {
	public static int countBST_method1(int n) {
        if(n==0||n==1)
            return 1;
		int[] dp = new int[n + 1];
		dp[0] = dp[1] = 1;
		//bottom-up
		for (int i = 2; i < dp.length; i++) {
			for (int j = 1; j <= i; j++) {
				dp[i] += (dp[i - j] * dp[j - 1]);
			}
		}
		return dp[n];
	}

	public static void main(String[] args) {

	}
}

package hacker.rank;

import java.util.Scanner;

public class BrickGame {
	
	class Profit{
		long first;
		long second;
	}
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int tst=sc.nextInt();
while(tst>0){
	int n=sc.nextInt();
	int A[]=new int[n];
	Profit dp[][]=new Profit[n][n];
	
	for (int i = 0; i < n; i++)
	{	dp[i][i].first=A[i];
		dp[i][i+1].first=A[i]+A[i+1];
		dp[i][i+2].first=A[i]+A[i+1]+A[i+2];
	}
	tst--;
}

}
}

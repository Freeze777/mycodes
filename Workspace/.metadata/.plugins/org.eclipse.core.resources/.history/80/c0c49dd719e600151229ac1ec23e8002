import java.util.Scanner;

import hacker.rank.ScalarProduct;


public class Knapsack {
  private int[][] M;
  private double[] weight;
  private long[] value;
  public Knapsack(double[] weight2, long[] b, int W) {
    assert weight2.length == b.length;
    this.weight = weight2;
    this.value = b;
    M = new int[weight2.length+1][W+1]; // i items with weight j
    for (int i=1;i<=weight2.length;i++)
      for (int j=1;j<=W;j++)
        M[i][j] = Integer.MIN_VALUE;
    System.out.println("Maximum value is " + getMaxValue(weight2.length, W));
  }

  private int getMaxValue(int i, double d) {
    if (i<=0 || d<=0)
      return 0;
    if (M[i][d] == Integer.MIN_VALUE)
    {
      M[i][d] = getMaxValue(i-1, d);
      if (d-weight[i-1] >= 0)
        M[i][d] = Math.max(M[i][d], getMaxValue(i-1, d-weight[i-1]) + value[i-1]);
    }
    assert M[i][d] != Integer.MIN_VALUE;
    return M[i][d];
  }

  public static void main(String[] args) {
	  
 Scanner sc=new Scanner(System.in);
 int tst=sc.nextInt();
 while(tst>0)
 {
	 int n=sc.nextInt();
	 int wt=sc.nextInt();
	 double[] wght=new double[n]; 
	 long[] b=new long[n];
	 for (int i = 0; i < args.length; i++) {
		wght[i]=sc.nextDouble();
	}
	 for (int i = 0; i < b.length; i++) {
		b[i]=sc.nextLong();
	}
	 new Knapsack(wght, b, wt);
	 tst--;
 }
    
   

  
  }

}
package algo.sort;

import java.util.Comparator;

public class ArrangeNumbersToFormBigNumber {
	public static void main(String[] args) {
		int[] arr={1,34,3,98,9,76,45,4};//998764543431
		//int[] arr={54,546,548,60};//6054854654
		String ans=getMaxNum(arr);
	}

	public static String getMaxNum(int[] arr) {
		Comparator<Integer> myComp=new Comparator<Integer>() {
			
			@Override
			public int compare(Integer o1, Integer o2) {
				// TODO Auto-generated method stub
				return o1.toString().concat(o2.toString()).compareTo(o2.toString().concat(o1.toString()));
			}
		};
		return null;
	}

}

package algo.trees;

public class TreeUtility {
	
	public static boolean isLeaf(Node root) {
		return ((root.left==null)&&(root.right==null));
	}

	public static void printIOT(Node root) {
		if (root == null)
			return;
		printIOT(root.left);
		System.out.print(root.data + " ");
		printIOT(root.right);
	}
}

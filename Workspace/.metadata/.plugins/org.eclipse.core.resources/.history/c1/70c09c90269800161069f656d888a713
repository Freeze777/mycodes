import java.util.*;

class TestClass {
	public static void main(String args[]) throws Exception {
		Scanner s = new Scanner(System.in);
		int l = s.nextInt();
		int m = s.nextInt();
		char[] str = s.next().toCharArray();
		int start = 0;
		if(m>=str.length){
			System.out.println("");
			System.exit(0);
		}
		
		while (m > 0) {
			char max = 0;
			int loc = -1;
			for (int i = start; i < start + m; i++) {
				if (str[i] > max) {
					loc = i;
					max = str[i];
				}
			}
			for (int i = start; i < loc; i++) {
				str[i] = '\0';
			}
			
			m -= (loc - start);
			start = loc+1;
		}
		for (int i = 0; i < str.length; i++) {
			if (str[i] != '\0')
				System.out.print(str[i]);
		}
	}
}

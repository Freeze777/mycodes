import java.util.Scanner;

class Vector{
	int x,y,z;

	public Vector(int x, int y, int z) {
		super();
		this.x = x;
		this.y = y;
		this.z = z;
	}
	public double angle(Vector v1,Vector v2){
		return (double)(v1.x*v2.x+v1.y*v2.y+v1.z*v2.z)/((Math.sqrt(v1.x*v1.x+v1.y*v1.y+v1.z*v1.z))*Math.sqrt(v1.x*v2.x+v1.y*v2.y+v1.z*v2.z));
		
	}
}

public class AnglesSpaces {
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		Vector[] v=new Vector[n];		
		while(n>0){
			int x=sc.nextInt();
			int y=sc.nextInt();
			int z=sc.nextInt();
			v[n-1]=new Vector(x,y,z);
			
			n--;
		}
		double sum=0;
		for (int i = 0; i < v.length; i++) {
			for (int j = i+1; j < v.length; j++) {
				for (int k =j+1; k < v.length; k++) {
					Vector v1=new Vector(v[j].z-v[i].z,v[j].y-v[i].y,v[j].z-v[i].z);
					Vector v1=new Vector(v[j].z-v[k].z,v[j].y-v[k].y,v[j].z-v[k].z);
					sum+=angle(v[j].x-v[i].x)
				}
				
			}
		}
		
		
	}

}

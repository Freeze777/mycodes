.container:before, .container:after {
    display:table;
    content:"";
    zoom:1 /* ie fix */;
}

.container:after {
    clear:both;
}

.container {
	top:700px;
    width:800px;
    margin:0 auto;
    border:1px solid ;
    box-shadow:0px 2px 7px #292929;
    -moz-box-shadow: 0px 2px 7px #292929;
    -webkit-box-shadow: 0px 2px 7px #292929;
    border-radius:10px;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
   
}

.mainbody {
	/* background-color:lightblue; */
	top:200px;
    height:400px;
    width:800px;
    border: solid #eee;
    border-width:1px 0;
	text-align: center;
}
